		ld a, (lf213h)
		or a
		jr nz, leb16h
		ld a, (lf217h)
		cp 0ffh
		jr nz, leb16h
		in a, (03fh)
		and 080h
		ret z
		call sub_eb50h
		ld (lf217h), a
		xor 0ffh
		ret z
leb16h:	
		ld a, 0ffh
		ret
